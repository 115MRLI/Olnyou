<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/menu_head"
        layout="@layout/mobile_terminal_menu"></include>

    <View
        android:id="@+id/menu_lin"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_below="@id/menu_head"
        android:layout_marginTop="5dp"
        android:background="@color/detailsstr" />

    <FrameLayout
        android:id="@+id/frame_skill"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/menu_lin">

    </FrameLayout>
</RelativeLayout>